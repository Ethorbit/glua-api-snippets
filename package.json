{
  "name": "glua-api-scraper",
  "description": "Scrapes the Garry's Mod Wiki in order to build EmmyLua comments that will provide intellisense help.",
  "keywords": [
    "Garry's Mod",
    "api",
    "EmmyLua"
  ],
  "homepage": "https://github.com/luttje/glua-api-snippets",
  "repository": {
    "type": "git",
    "url": "https://github.com/luttje/glua-api-snippets"
  },
  "author": "Luttje",
  "license": "MIT",
  "version": "0.2.0",
  "type": "module",
  "main": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "bin": {
    "videobrew": "./dist/esm/cli.js"
  },
  "scripts": {
    "dev": "npx tsc --watch",
    "watch": "npx tsc --watch",
    "build": "npx tsc",
    "cli": "npx ts-node-esm ./src/cli.ts",
    "cli:scrape-wiki": "npm run cli -- --output ./output/wiki.json",
    "test": "npx jest",
    "ci:test": "npm run test -- --ci --coverage"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "html-loader": "^4.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-fetch-mock": "^3.0.3",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.1.0",
    "ts-jest-resolver": "^2.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "gitHead": "2513190cc1de00d1049e35f9c942b779447530f9"
}
